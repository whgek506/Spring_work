package user.bean;

import org.springframework.jdbc.core.namedparam.SqlParameterSource;

import lombok.Getter;
import lombok.Setter;

@Setter
public class UserDTO {
	private String name;
	private String id;
	private String pwd;
	
	@Override
	public String toString() {
		return name + "\t" + id + "\t" +pwd;
	}
}
